    "license": "MIT",
    "authors": [
        {
            "name": "Ross Keatinge",
            "email": "tetranz@gmail.com"
        },
        {
            "name": "Patrick Decoster",
            "email": "p.decoster@dev7.nl"
        },
          
    ],
    "require": {
        "symfony/symfony": ">=2.2",
        "doctrine/doctrine.odm": ">=1.0"
    },
    "autoload": {
        "psr-0": { "Zangai\\Select2DocumentBundle": "" }
    },
    "target-dir": "Zangai/Select2DocumentBundle",
    "extra": {
        "branch-alias": {
        }
    }
